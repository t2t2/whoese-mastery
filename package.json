{
  "name": "whose-mastery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack -d --progress --color",
    "build:prod": "webpack -p --progress --color --env.prod 1",
    "dev": "npm-run-all --parallel dev:server dev:webpack",
    "dev:server": "cross-env NODE_ENV=development NODE_CONFIG='{\\\"dev\\\":true}' nodemon --exec babel-node src/start.js",
    "dev:webpack": "babel-node bin/dev-server.js",
    "lint": "xo --ext .js,.vue"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-node5": "^11.0.2",
    "babel-runtime": "^5.8.38",
    "body-parser": "^1.15.0",
    "bulma": "0.0.24",
    "config": "^1.20.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "feathers": "^2.0.0",
    "feathers-hooks": "^1.5.2",
    "feathers-rest": "^1.2.5",
    "feathers-socketio": "^1.3.4",
    "file-loader": "^0.8.5",
    "lodash": "^4.11.1",
    "motion-ui": "^1.2.2",
    "node-sass": "^3.6.0",
    "nunjucks": "^2.4.2",
    "sass-loader": "^3.2.0",
    "sass-material-colors": "0.0.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^1.0.21",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.2.3",
    "vue-router": "^0.7.13",
    "vue-style-loader": "^1.0.0",
    "vue-syncers-feathers": "^0.2.0",
    "webpack": "^2.1.0-beta.6",
    "webpack-manifest-plugin": "danethurber/webpack-manifest-plugin#unamed-chunks"
  },
  "devDependencies": {
    "cross-env": "^1.0.7",
    "eslint-plugin-html": "^1.4.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "http-proxy-middleware": "^0.14.0",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.8.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-notifier": "^1.3.0",
    "xo": "^0.14.0"
  },
  "xo": {
    "exnext": true,
    "ignores": [],
    "plugins": [
      "html"
    ],
    "semicolon": false
  }
}
